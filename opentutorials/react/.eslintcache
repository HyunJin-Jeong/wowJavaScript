Submit={function(){\n      }.bind(this)}></UpdateContent>\n    }\n    return article;\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        <Subject\n          title={this.state.subject.title}\n          subTitle={this.state.subject.subTitle}\n          onChangePage={function() {\n            this.setState({mode: \"welcome\"});\n          }.bind(this)}\n        >\n        </Subject>\n        <TOC\n          data={this.state.contents}\n          onChangePage={function(id) {\n            this.setState({\n              mode: \"read\",\n              selectedID: Number(id),\n            });\n          }.bind(this)}\n        >\n        </TOC>\n        <Control onChangeMode={function(mode) {\n          this.setState({mode})\n        }.bind(this)}></Control>\n        {this.getContent()}\n      </div>\n    );\n  }\n}\n\nexport default App;","C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\reportWebVitals.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\toc.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\subject.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\TOC.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\Subject.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\Control.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\ReadContent.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\components\\CreateContent.js",[],"C:\\Users\\hyun0\\OneDrive\\Main Folder\\Study\\Programming\\wowPractice\\opentutorials\\react\\src\\co